.title{
  width: 750rpx;
  height: 150rpx;
  background-color: rgba(105, 105, 105, 0.717);
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
}
.tleft{
  width: 70%;
  display: flex;
  flex-direction: column;
}
.tright{
  display: flex;
  flex-direction: column;
  margin-left: 100rpx;
  color: aliceblue;
}
.text1{
  color: aliceblue;
  margin-left: 30rpx;
  margin-top: 10rpx;
}
.text2{
  color: rgba(214, 219, 222, 0.614);
  margin-left: 30rpx;
  margin-top: 30rpx;
  font-size: 12px;
}
.text3{
  margin-top: 20rpx;

}
.text4{
  font-size: 22px;
  font-weight: bold;
}

.left {
  /* width: 100%;
  height: 100%; */
  display: flex;
  flex-wrap: wrap;
  margin: 10rpx 10rpx;
}
.left navigator{
  width: 48%;
  margin-right: 5rpx
}
.left image{
  width: 100%;
  border-radius: 10rpx;
}

.text{
  font-size: 11px;
}
.images{
  background-color: aliceblue;
  margin-bottom: 10rpx;
}
.count{
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center
}
.count image{
  width: 30rpx;
  height: 30rpx;
  margin-right:20rpx;
  margin-left: 200rpx
}
.count text{
  font-size: 11px
}